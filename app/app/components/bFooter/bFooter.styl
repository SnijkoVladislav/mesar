.h-footer
	padding 50px
	&__item
		&._size-1in3
			span(1/3)

.b-copywrite
	font-weight 800
	font-size 14px
	text-transform uppercase
	letter-spacing 3px
	margin-top 10px

.b-beack-to-top
	cursor pointer
	margin 0px auto
	float none
	width 105px
	display block
	color #222
	text-decoration none

.b-social
	float right
	margin 0px
	padding 0px
	&__item
		margin 0px 5px
		display inline-block
		cursor pointer
		list-style-type none
		width 32px
		height 32px
		background-size contain

._fasebook
	background-image url('data:image/png;base64,')

._twitter
	background-image url('data:image/png;base64,')

._dribbble
	background-image url('data:image/png;base64,')

