.h-services
	background-color #ffdb43
	cf()
	&__header
		padding 15px 25px
	&__content
		cf()
		padding 0px 50px 0px

.b-title
	display inline-block
	text-transform uppercase
	font-weight 800
	background #000
	color #fff
	padding 5px 6px
	letter-spacing 5px
	font-size 15px
	font-family 'Open Sans', sans-serif

.b-services-item
	col(1/4, gutter:10, cycle:4)
	text-align center
	padding-bottom 10px
	padding-top 10px
	transition all .25s linear
	&:hover
		background-color #EFCD3E

	&__img
		display inline-block

	&__title
		font-size 18px
		text-transform uppercase
		font-weight 800
		margin-top 0
		padding-top 0
		margin-bottom 0
		padding-bottom 0

	&__desc
		color #222
		letter-spacing 1px
		font-family 'Gentium Basic', serif
		line-height 30px
		font-size 15px

